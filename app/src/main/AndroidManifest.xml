<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.imitation_wechat">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_wechat"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_wechat_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NichengActivity"></activity>
        <activity android:name=".PersonalInfoActivity" />
        <activity android:name=".ChatMoreActivity" />
        <activity android:name=".ChatActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>